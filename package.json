{
  "name": "rollup-plugin-executable",
  "version": "1.2.0",
  "description": "The Rollup Executable Plugin changes output javascript file's executable state to ON on unix like operating systems.",
  "keywords": [
    "rollup-plugin",
    "file",
    "executable"
  ],
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0",
    "yarn": ">=1.0.0"
  },
  "author": {
    "name": "Sebastian Fastner",
    "email": "s.fastner@sebastian-software.de",
    "url": "http://sebastian-software.de/fastner"
  },
  "license": "MIT",
  "main": "lib/node.cjs.js",
  "module": "lib/node.esm.js",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore .",
    "fix:js": "eslint --ignore-path .gitignore --fix .",
    "format:js": "prettier --ignore-path .gitignore --write '**/*.{js,mjs}' && npm run fix:js",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "npm run prepublish && ava --serial",
    "prepublish": "rimraf lib && prepublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/rollup-plugin-executable.git"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-core/register"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-preset-edge": "^4.10.1",
    "denodeify": "^1.2.1",
    "eslint-config-readable": "^2.1.4",
    "prepublish": "^2.1.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1"
  }
}
