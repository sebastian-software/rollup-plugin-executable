{
  "name": "rollup-plugin-executable",
  "version": "1.5.1",
  "description": "The Rollup Executable Plugin changes output javascript file's executable state to ON on unix like operating systems.",
  "keywords": [
    "rollup-plugin",
    "file",
    "executable"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=4.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "lib"
  ],
  "author": {
    "name": "Sebastian Fastner",
    "email": "s.fastner@sebastian-software.de",
    "url": "http://sebastian-software.de/fastner"
  },
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "format:js": "prettier --write '**/*.js' && npm run lint:fix",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "jest --coverage",
    "prepare": "rimraf lib && preppy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/rollup-plugin-executable.git"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2"
  },
  "devDependencies": {
    "@babel/register": "^7.6.2",
    "@effective/eslint-config": "^1.1.1",
    "@types/jest": "^24.0.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-preset-edge": "^5.5.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "preppy": "^8.3.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.22.0",
    "typescript": "^3.6.3"
  }
}
