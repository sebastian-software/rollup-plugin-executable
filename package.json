{
  "name": "rollup-plugin-executable",
  "version": "0.0.1",
  "description": "The Rollup Rebase Plugin changes output javascript file's executable state to ON on unix like operating systems.",
  "keywords": [
    "rollup-plugin",
    "file",
    "executable"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Sebastian Fastner",
  "license": "MIT",
  "main": "lib/node.commonjs.js",
  "jsnext:main": "lib/node.esmodule.js",
  "module": "lib/node.esmodule.js",
  "scripts": {
    "lint": "eslint --quiet src/ test/ || true",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "npm run prepublish && ava --serial",
    "prepublish": "prepublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/rollup-plugin-executable.git"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-core/register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "espower",
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.20.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.15.0",
    "prepublish": "^0.12.2",
    "rollup": "^0.41.1",
    "s15e-javascript": "^0.7.11"
  }
}
