{
  "name": "rollup-plugin-executable",
  "version": "1.4.6",
  "description": "The Rollup Executable Plugin changes output javascript file's executable state to ON on unix like operating systems.",
  "keywords": [
    "rollup-plugin",
    "file",
    "executable"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=4.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "lib"
  ],
  "author": {
    "name": "Sebastian Fastner",
    "email": "s.fastner@sebastian-software.de",
    "url": "http://sebastian-software.de/fastner"
  },
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "scripts": {
    "lint:js": "eslint .",
    "fix:js": "eslint --fix .",
    "format:js": "prettier --write '**/*.js' && npm run fix:js",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "npm run prepublish && jest --coverage",
    "prepublish": "rimraf lib && preppy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/rollup-plugin-executable.git"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5"
  },
  "devDependencies": {
    "@babel/register": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-preset-edge": "^5.3.0",
    "denodeify": "^1.2.1",
    "eslint-config-readable": "^2.3.2",
    "jest": "^24.8.0",
    "preppy": "^8.1.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.1"
  }
}
